<?xml version="1.0" encoding="UTF-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >

    <PreferenceCategory android:title="@string/touchscreen_subcat_title" >
        <CheckBoxPreference
            android:key="use_sweep2wake"
            android:summary="@string/use_sweep2wake_summary_head"
            android:title="@string/use_sweep2wake_title_head" />
        <com.cyanogenmod.settings.device.S2WThreshold
            android:key="x_sweep2wake"
            android:summary="@string/x_sweep2wake_summary_head"
            android:title="@string/x_sweep2wake_title_head"
            android:digits="123456789"
            android:inputType="number"
            android:defaultValue="120" />
        <com.cyanogenmod.settings.device.S2WThreshold
            android:key="y_sweep2wake"
            android:summary="@string/y_sweep2wake_summary_head"
            android:title="@string/y_sweep2wake_title_head"
            android:digits="123456789"
            android:inputType="number"
            android:defaultValue="240" />
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/scheduler_subcat_title" >
        <com.cyanogenmod.settings.device.SchedMC
            android:defaultValue="0"
            android:entries="@array/sched_mc_entries"
            android:entryValues="@array/sched_mc_entries_values"
            android:key="sched_mc"
            android:summary="@string/sched_mc_summary_head"
            android:title="@string/sched_mc_title_head" />
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/deepsleep_subcat_title" >
        <com.cyanogenmod.settings.device.DeepestSleepState
            android:defaultValue="3"
            android:entries="@array/deepest_sleep_states_entries"
            android:entryValues="@array/deepest_sleep_states_entries_values"
            android:key="deepest_sleep_state"
            android:summary="@string/deepest_sleep_summary_head"
            android:title="@string/deepest_sleep_title_head" />
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/fsync_subcat_title" >
        <com.cyanogenmod.settings.device.FsyncMode
            android:defaultValue="0"
            android:entries="@array/fsync_mode_entries"
            android:entryValues="@array/fsync_mode_entries_values"
            android:key="fsync_mode"
            android:summary="@string/fsync_summary_head"
            android:title="@string/fsync_title_head" />
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/tcp_subcat_title" >
        <com.cyanogenmod.settings.device.TCPControl
            android:defaultValue="cubic"
            android:entries="@array/tcp_control_entries"
            android:entryValues="@array/tcp_control_entries_values"
            android:key="tcp_control"
            android:summary="@string/tcp_summary_head"
            android:title="@string/tcp_title_head" />
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/gpu_subcat_title" >
        <com.cyanogenmod.settings.device.MaliL2MaxReads
            android:defaultValue="48"
            android:entries="@array/mali_l2_mr_entries"
            android:entryValues="@array/mali_l2_mr_entries_values"
            android:key="mali_l2_mr"
            android:summary="@string/mali_l2_mr_summary_head"
            android:title="@string/mali_l2_mr_title_head" />

        <com.cyanogenmod.settings.device.MaliPreAllocMem
            android:defaultValue="50331648"
            android:entries="@array/mali_pam_entries"
            android:entryValues="@array/mali_pam_entries_values"
            android:key="mali_pam"
            android:summary="@string/mali_pam_summary_head"
            android:title="@string/mali_pam_title_head" />
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/mmc_subcat_title" >
        <CheckBoxPreference
            android:key="use_spi_crc"
            android:summary="@string/use_spi_crc_summary_head"
            android:title="@string/use_spi_crc_title_head" />
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/wifi_subcat_title" >
        <CheckBoxPreference
            android:key="use_wifipm_max"
            android:summary="@string/use_wifipm_max_summary_head"
            android:title="@string/use_wifipm_max_summary_head" />
    </PreferenceCategory>

</PreferenceScreen>
